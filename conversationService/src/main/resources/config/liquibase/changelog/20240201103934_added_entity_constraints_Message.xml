<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Message.
    -->
    <changeSet id="20240201103934-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="repsonse_id"
                                 baseTableName="message"
                                 constraintName="fk_message__repsonse_id"
                                 referencedColumnNames="id"
                                 referencedTableName="message"
                                 />

        <addForeignKeyConstraint baseColumnNames="actor_id"
                                 baseTableName="message"
                                 constraintName="fk_message__actor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="actor"
                                 />
    </changeSet>
</databaseChangeLog>
